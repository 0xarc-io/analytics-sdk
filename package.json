  },
  "author": "Yaraslau Stsetskevich <j@arcx.money>",
  "main": "dist/cjs/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "module": "dist/esm/src/index.ts",
  "unpkg": "dist/index.umd.min.js",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@restless/sanitizers": "^0.2.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.9",
    "@types/sinon": "^10.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "prepare": "yarn lint && yarn build",
    "prebuild": "yarn clean",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish",
    "build": "yarn build:cjs && yarn build:esm && yarn webpack --mode=production",
    "build:cjs": "yarn tsc --build",
    "build:esm": "yarn tsc -m es6 --outdir dist/esm",
    "clean": "rm -rf dist",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts'"
